{% if type is same as ('Modele')  %}

{% for modele in results %}
  <li>
   <a href="{{path('home')}}" > {{modele.getBrand().getBrandName()}} {{ modele.modele}} </a>
  </li>
{% endfor %}

{% elseif type is same as ('Club') %}

{% for club in results %}
  <li>
   <a href="{{path('home')}}" > {{ club.clubName}} </a>
  </li>
{% endfor %}

{% elseif type is same as ('Event') %}

{% for event in results %}
  <li>
   <a href="{{path('home')}}" > {{event.eventName}} </a>
  </li>
{% endfor %}

{% endif %}
