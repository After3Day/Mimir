<div class="input-field col s12">
  <select id="testInTest">
    <option value="0" disabled selected >Choisir un collectionneur</option>
    {% for collector in results %}
    <option value="{{collector.id}}">{{ collector.contact.name }} {{ collector.contact.surName }}</option>
    {% endfor %}
  </select>
  <label>Collectionneurs</label>
</div>

<script type="text/javascript">
$(document).ready(function() {
  $('select').formSelect();
});
</script>
