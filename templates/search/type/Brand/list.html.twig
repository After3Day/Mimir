<div class="input-field col s12 m6">
  <select id="testInTest">
    <option value="0" disabled selected>Choisir une marque</option>
    {% for brand in results %}
      {% if brand.isModelesExist %}
        <option value="{{brand.id}}">{{ brand.brandName}}</option>
      {% endif %}
    {% endfor %}
  </select>
  <label>Marque</label>
</div>

<script type="text/javascript">
  $(document).ready(function() {
  $('select').formSelect();
});

</script>

