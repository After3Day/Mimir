  <hr>

    <form class="homeForm" action="" method="post">
      <div class="form-group">
        <div class="input-group mb-3">
          <input type="hidden" id="urlAjax" class="form-control" value="{{path('edit')}}"/>

          <div class="input-group-append">
            <select id="editTest" class="custom-select" >
              <option value="0">Choisir une option</option>
              <option value="Brand">Voiture</option>
              <option value="Club">Club</option>
              <option value="Event">Event</option>
              <option value="Collector">Collector</option>
              <option value="Designer">Designer</option>
            </select>
          </div>

        </div>
      </div>
    </form>
    <hr>
</div>

<div id="edit"></div>

{% endblock %}

{% block javascripts %}
  <script type="text/javascript" src="{{ asset('js/edit.js') }}"></script>
{% endblock %}
