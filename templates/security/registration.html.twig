{% extends 'base.html.twig' %}

{% block body %}

  <h1>Inscritpion sur le site</h1>

  {{ form_start(form) }}

  {{ form_row(form.userName, {'label': "Nom d'utilisateur",'attr': {'placeholder':'UserName'}}) }}
  {{ form_row(form.password, {'label': "Mot de passe",'attr': {'placeholder':'Mot de passe'}}) }}
  {{ form_row(form.confirm_password, {'label': "Confirmer le mot de passe",'attr': {'placeholder':'Confirmation'}}) }}

  <button class="btn btn-success">Inscritpion</button>
  {{ form_end(form) }}

{% endblock %}
